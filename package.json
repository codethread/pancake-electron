{
  "name": "pancake",
  "version": "0.1.0",
  "description": "A Pull Request Dashboard on your Desktop",
  "main": "./build/main.js",
  "scripts": {
    "clean": "git clean -dfx && yarn install",
    "dev": "FORCE_COLOR=1 run-p --print-label dev:*",
    "machine": "xstate-codegen \"client/machines/**/*Machine.ts\"",
    "machine:once": "yarn machine --once",
    "storybook": "start-storybook -p 6006 -s public -c tooling/storybook",
    "lint": "eslint . --cache",
    "test": "jest --coverage",
    "visual": "backstop test --config=tooling/backstop/config.js --docker",
    "visual:approve": "backstop approve --config=tooling/backstop/config.js",
    "visual:once": "sh backstop.sh",
    "e2e": "jest e2e --modulePathIgnorePatterns='' --no-coverage --maxWorkers 1",
    "docker-e2e": "docker build . -f Dockerfile -t pancake",
    "build": "FORCE_COLOR=1 run-p --print-label build:*",
    "start": "electron .",
    "checks": "FORCE_COLOR=1 npm-run-all --print-label --parallel lint test --serial build --parallel e2e visual:once",
    "build:electron": "webpack --config tooling/webpack/electron.webpack.js --mode production",
    "build:react": "webpack --config tooling/webpack/react.webpack.js --mode production",
    "dev:electron": "wait-on http-get://localhost:4000/ && cross-env NODE_ENV=development webpack --config tooling/webpack/electron.webpack.js --mode development && yarn start",
    "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config tooling/webpack/react.webpack.js --mode development",
    "prerelease": "npm-run-all clean checks",
    "release": "electron-builder --mac",
    "postinstall": "electron-builder install-app-deps",
    "graphql": "graphql-codegen --config tooling/graphql/codegen.yml"
  },
  "keywords": [],
  "author": "Adam Hall <adamhalldesigns@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.13.8",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.13.8",
    "@babel/plugin-proposal-optional-chaining": "7.13.8",
    "@babel/preset-env": "7.13.9",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.13.0",
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-document-nodes": "1.17.12",
    "@graphql-codegen/typescript-operations": "1.18.0",
    "@storybook/addon-actions": "6.1.21",
    "@storybook/addon-essentials": "6.1.21",
    "@storybook/addon-links": "6.1.21",
    "@storybook/addons": "6.1.21",
    "@storybook/react": "6.1.21",
    "@storybook/theming": "6.1.21",
    "@testing-library/dom": "7.30.4",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "13.1.8",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/jest": "26.0.20",
    "@types/lodash.merge": "4.6.6",
    "@types/node": "12.19.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/react-router-dom": "5.1.7",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "1.12.0",
    "backstopjs": "5.1.0",
    "cross-env": "7.0.3",
    "electron": "11.3.0",
    "electron-builder": "22.7.0",
    "electron-builder-notarize": "1.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "3.8.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "4.1.1",
    "fast-glob": "3.2.5",
    "graphql": "15.5.0",
    "html-webpack-plugin": "4.5.2",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "spectron": "13.0.0",
    "ts-jest": "26.5.2",
    "typescript": "4.2.2",
    "wait-on": "5.2.1",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2",
    "ws": "7.4.4"
  },
  "dependencies": {
    "@xstate/inspect": "0.4.1",
    "@xstate/react": "1.3.1",
    "electron-devtools-installer": "3.1.1",
    "electron-log": "4.3.2",
    "electron-updater": "4.3.5",
    "got": "11.8.2",
    "lodash.merge": "4.6.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-is": "17.0.2",
    "react-router-dom": "5.2.0",
    "styled-components": "5.2.1",
    "xstate": "4.17.0",
    "xstate-codegen": "0.1.0-next.8"
  },
  "build": {
    "appId": "com.electron.pancake",
    "productName": "Pancake",
    "afterSign": "electron-builder-notarize",
    "linux": {
      "category": "Utility"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "entitlements": "./publishingTools/entitlements.mac.plist",
      "entitlementsInherit": "./publishingTools/entitlements.mac.plist",
      "hardenedRuntime": true
    },
    "directories": {
      "output": "dist",
      "buildResources": "publishingTools"
    },
    "files": [
      "package.json",
      "publishingTools/icon.*",
      "build/**"
    ]
  },
  "volta": {
    "node": "12.18.3",
    "yarn": "1.22.10"
  }
}
