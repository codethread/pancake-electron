{
  "name": "Pancake",
  "version": "0.0.1",
  "description": "A Pull Request Dashboard on your Desktop",
  "main": "./build/main.js",
  "scripts": {
    "dev": "run-p dev:*",
    "lint": "eslint .",
    "test": "jest",
    "e2e": "jest e2e --modulePathIgnorePatterns='' --no-coverage",
    "docker-e2e": "docker build . -f Dockerfile -t pancake",
    "build": "run-p build:*",
    "start": "electron .",
    "build:electron": "webpack --config tooling/electron.webpack.js --mode production",
    "build:react": "webpack --config tooling/react.webpack.js --mode production",
    "dev:electron": "wait-on http-get://localhost:4000/ && cross-env NODE_ENV=development webpack --config tooling/electron.webpack.js --mode development && yarn start",
    "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config tooling/react.webpack.js --mode development",
    "checks": "npm-run-all build --parallel lint test e2e",
    "release": "electron-builder --mac --linux",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [],
  "author": "Adam Hall <adamhalldesigns@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.13.8",
    "@babel/plugin-proposal-optional-chaining": "7.13.8",
    "@babel/preset-env": "7.13.9",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.13.0",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/jest": "26.0.20",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "babel-loader": "8.2.2",
    "cross-env": "7.0.3",
    "electron": "11.3.0",
    "electron-builder": "22.10.5",
    "electron-builder-notarize": "1.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "3.6.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-testing-library": "3.10.1",
    "html-webpack-plugin": "4.5.2",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "spectron": "13.0.0",
    "ts-jest": "26.5.2",
    "typescript": "4.2.2",
    "wait-on": "5.2.1",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "electron-devtools-installer": "3.1.1",
    "electron-log": "4.3.2",
    "electron-updater": "4.3.8",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "styled-components": "^5.1.1"
  },
  "build": {
    "appId": "com.electron.pancake",
    "afterSign": "electron-builder-notarize",
    "linux": {
      "category": "Utility"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "entitlements": "./publishingTools/entitlements.mac.plist",
      "entitlementsInherit": "./publishingTools/entitlements.mac.plist",
      "hardenedRuntime": true
    },
    "directories": {
      "output": "dist",
      "buildResources": "publishingTools"
    },
    "files": [
      "package.json",
      "publishingTools/icon.*",
      "build/**"
    ]
  },
  "volta": {
    "node": "12.18.3",
    "yarn": "1.22.10"
  }
}
